{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "layout": "Conceptual",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
	  "contributors_to_exclude": ["hexiaokai", "openpublishingbuild","sudeepku","saldana"]
    },
    "fileMetadata": {},
    "template": [],
    "dest": "docs"
  }
}
